    "@emotion/styled": "^11.6.0",
    "@fontsource/ibm-plex-mono": "^4.5.1",
    "@fontsource/ibm-plex-sans": "^4.5.1",
    "@mdx-js/react": "^1.6.22",
    "next": "^12.0.7",
    "next-seo": "^4.28.1",
    "nextra": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.2"
